div.timeline-frame {  
    border-color: #5D99C3!important; 
    border-radius: 5px!important; 
} 
  
div.timeline-axis {  
    border-color: #5D99C3!important;  
    background-color: #5D99C3!important;  
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#5D99C3', endColorstr='#3A6DA0') alpha(opacity = 100)!important; 
    background: -webkit-gradient(linear, left top, left bottom, from(#5D99C3), to(#3A6DA0))!important;  
    background: -moz-linear-gradient(top,  #5D99C3,  #3A6DA0)!important;  
    -khtml-opacity: 1!important;  
    -moz-opacity: 1!important;  
    opacity: 1!important;  
}  
  
div.timeline-groups-axis {  
    border-color: #5D99C3!important;  
}  
  
div.timeline-groups-axis-onleft {  
    border-style: none solid none none!important;  
}  
  
div.timeline-axis-text {  
    color: white!important;  
}  
  
div.timeline-event {  
    color: white !important;  
    border-radius: 5px !important;  
}  
  
div.timeline-event-content {  
    padding: 5px!important; 
    text-shadow: none!important; 
}  
  
div.unavailable {  
    background: #F03030 none !important;  /* red */  
    border-color: #bd2828 !important;     /* red */  
}  
  
div.available {  
    background: #1AA11A none !important;  /* green */  
    border-color: #136e13 !important;     /* green */  
}  
  
div.maybe {  
    background: #FFA500 none !important;  /* orange */  
    border-color: #cc8100 !important;     /* orange */  
}  
  
div.timeline-event-selected {  
    background: #BECEFE none !important;  
    border-color: #97B0F8 !important;  
}  