		<h:form id="form2">
			<div class="col-md-4">

				<div class="widget widget-table">
					<div class="widget-header">
						<h3>
							<i class="fa fa-users"></i>Liste des employées
						</h3>
					</div>
					<div class="widget-content">

						<div class="input-group input-group-sm">
							<h:form id="formSearch">
								<h:outputLabel value="Matricule:" />
								<p:inputText value="#{occurenceBean.matricul}"
									onkeypress="if (event.keyCode == 13) {onchange(); return false; }">
									
								</p:inputText>

							</h:form>
						</div>
						<br />




						<p:dataTable id="eventsDT" var="identite"
							value="#{occurenceBean.identites}" selectionMode="single"
							selection="#{occurenceBean.selectedIdentite}"
							rowKey="#{identite.numedoss}">

							<f:facet name="header"> List of Employers </f:facet>
							<p:ajax event="rowSelect"
								listener="#{occurenceBean.onRowSelect}" update=":form1" />



							<p:column headerText="Matricule">
								<h:outputText value="#{identite.matricul}" />
							</p:column>
							<p:column headerText="Nom et Prénom">
								<h:outputText value="#{identite.nom} #{identite.prenom}" />
							</p:column>
						</p:dataTable>
					</div>

				</div>
			</div>

		</h:form>







