			</h:form>
		</p:dialog>

		<!-- ////////// 
							Popup de modification de terminal de pointage  ////////// -->

		<p:dialog header="Modification du terminal de pointage"
			widgetVar="DialogEditTerminauxPointage" modal="true">
			<h:form id="formDialogEditTerminauxPointage">
				<h:panelGrid columns="2" cellpadding="10">
					<p:outputLabel value="Code" />
					<p:inputText value="#{terminalPointageBean.selectedTerminal.code}"
						required="true" />

					<p:outputLabel value="Libelle" />
					<p:inputText
						value="#{terminalPointageBean.selectedTerminal.libelle}"
						required="true" />

				</h:panelGrid>
				<br />
				<p:commandButton value="Sauvegarder"
					update=":form1:terminauxPointageDataTable"
					action="#{terminalPointageBean.modifierTypePointage()}"
					oncomplete="PF('DialogEditTerminauxPointage').hide()" />
			</h:form>
		</p:dialog>


	</ui:define>
</ui:composition>
</html>
