<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:html5="http://java.sun.com/jsf/passthrough"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:p="http://primefaces.org/ui">

<ui:composition template="/template/MainContent.xhtml">

	<ui:define name="Content">
		<h:form id="form1">
			<!-- WIDGET TABBED CONTENT -->

			<div class="col-md-8" style="width: 85%">
				<div class="widget">
					<div class="widget-header-in">

						<ul class="nav nav-tabs pull-right">
							<li><button class="btn btn-default  btn-primary ">
									<i class="fa fa-file-pdf-o"></i> Export to pdf
								</button></li>
							<li><button class="btn btn-default btn-primary ">
									<i class="fa fa-print"></i> Print
								</button></li>
						</ul>

						<ul class="nav nav-tabs">

							<li class="active"><a href="#tab1" data-toggle="tab"><i
									class="fa fa-cubes"></i><span>Activités</span></a></li>
							<li><a href="#tab2" data-toggle="tab"><i
									class="fa fa-folder"></i><span>Groupe Activités</span></a></li>
							<li><a href="#tab3" data-toggle="tab"><i
									class="fa fa-credit-card"></i><span>Imputations</span></a></li>
							<li><a href="#tab4" data-toggle="tab"><i
									class="fa fa-credit-card"></i><span>Groupe Imputations</span></a></li>
							<li><a href="#tab5" data-toggle="tab"><i
									class="fa fa-credit-card"></i><span>Type Pointage</span></a></li>
							<li><a href="#tab6" data-toggle="tab"><i
									class="fa fa-credit-card"></i><span>Terminaux Pointage</span></a></li>

						</ul>

					</div>

					<div class="widget-content">
						<div class="tab-content no-padding">
							<p:messages showDetail="true" autoUpdate="true" closable="true"
								id="msg1" style="position: fixed; align: center; width: 65%" />
							<div class="tab-pane fade in active" id="tab1">

								<!-- ////////// Debut  Gestion des activités de type presence  ////////// -->

								<p:panel header="Activités en presence" style="border:0px">

									<p:dataTable id="activpretable"
										value="#{activitePresenceBean.activitePresences}"
										var="activpres"
										selection="#{activitePresenceBean.selectedActivitePresence}"
										rowKey="#{activpres.idap}" selectionMode="single"
										paginator="true"
										paginatorTemplate="{PreviousPageLink} {NextPageLink} {RowsPerPageDropdown}"
										rowsPerPageTemplate="3,5,10" paginatorPosition="top" rows="5">

										<p:ajax event="rowSelect"
											listener="#{activitePresenceBean.onRowSelectDataTable()}"
											update="activpretable" />

										<f:facet name="header" id="headerButtonActPre">

											<p:commandButton icon="ui-icon-zoomin"
												onclick="PF('DialogViewActPre').show()"
												update=":formDialogViewActPre"
												disabled="#{activitePresenceBean.headerButtonsDisabled}" />

											<p:commandButton icon="ui-icon-plus"
												onclick="PF('DialogAddActPre').show();"
												update=":formDialogAddActPre" />

											<p:commandButton icon="ui-icon-pencil"
												onclick="PF('DialogEditActPre').show();"
												update=":formDialogEditActPre"
												disabled="#{activitePresenceBean.headerButtonsDisabled}" />

											<p:commandButton icon="ui-icon-trash" ajax="true"
												action="#{activitePresenceBean.supprimerActivitePresence()}"
												update="activpretable"
												disabled="#{activitePresenceBean.headerButtonsDisabled}" />

										</f:facet>

										<p:column headerText="Code" style="text-align: center;">
											<h:outputText value="#{activpres.codeap}" />
										</p:column>
										<p:column headerText="Libellé" style="text-align: center;">
											<h:outputText value="#{activpres.libelles.get(0).lib}" />
										</p:column>
										<p:column headerText="Poids" style="text-align: center;">
											<h:outputText value="#{activpres.poids}" />
										</p:column>

									</p:dataTable>
								</p:panel>

								<!-- //////////  Fin  Gestion des activités en presence ////////// -->

								<!-- ************************************************************************************ -->

								<!-- ////////// Debut Gestion des activités de type Absence  ////////// -->

								<p:panel header="Activités en Absence" id="panelActiviteAbs"
									style="border:0px">
									<p:dataTable id="activabstable"
										value="#{activiteAbsenceBean.activiteAbsences}" var="activabs"
										selection="#{activiteAbsenceBean.selectedActiviteAbsence}"
										rowKey="#{activabs.idaabs}" selectionMode="single"
										paginator="true"
										paginatorTemplate="{PreviousPageLink} {NextPageLink} {RowsPerPageDropdown}"
										rowsPerPageTemplate="3,5,10" paginatorPosition="top" rows="5">

										<p:ajax event="rowSelect"
											listener="#{activiteAbsenceBean.onRowSelectDataTable()}"
											update="activabstable" />

										<f:facet name="header">

											<p:commandButton icon="ui-icon-zoomin"
												onclick="PF('DialogViewActAbs').show()"
												update=":formDialogViewActAbs"
												disabled="#{activiteAbsenceBean.headerButtonsDisabled}" />

											<p:commandButton icon="ui-icon-plus"
												onclick="PF('DialogAddActAbs').show();"
												update=":formDialogAddActAbs" />

											<p:commandButton icon="ui-icon-pencil"
												onclick="PF('DialogEditActAbs').show();"
												update=":formDialogEditActAbs"
												disabled="#{activiteAbsenceBean.headerButtonsDisabled}" />

											<p:commandButton icon="ui-icon-trash" ajax="true"
												action="#{activiteAbsenceBean.supprimerActiviteAbsence()}"
												update="activabstable"
												disabled="#{activiteAbsenceBean.headerButtonsDisabled}" />

										</f:facet>

										<p:column headerText="Code" style="text-align: center;">
											<h:outputText value="#{activabs.codeaabs}" />
										</p:column>
										<p:column headerText="Libellé" style="text-align: center;">
											<h:outputText value="#{activabs.libelles.get(0).lib}" />
										</p:column>
										<p:column headerText="Poids" style="text-align: center;">
											<h:outputText value="#{activabs.poids}" />
										</p:column>

									</p:dataTable>
								</p:panel>

								<!-- //////////  Fin  Gestion des activités en Absence ////////// -->
							</div>

							<div class="tab-pane fade" id="tab2">

								<!-- //////////  Debut Gestion groupes d'activités ////////// -->

								<p:panel header="Groupes activités" style="border:0px;">
									<p:dataTable id="groupeActiviteDataTable"
										value="#{groupeActiviteBean.groupeActivites}" var="grpAct"
										selection="#{groupeActiviteBean.selectedGroupeActivite}"
										rowKey="#{grpAct.idgrpa}" selectionMode="single"
										paginator="true"
										paginatorTemplate="{PreviousPageLink} {NextPageLink} {RowsPerPageDropdown}"
										rowsPerPageTemplate="3,5,10" paginatorPosition="top" rows="5">

										<p:ajax event="rowSelect"
											listener="#{groupeActiviteBean.onRowSelectDataTable()}"
											update="groupeActiviteDataTable" />

										<f:facet name="header">

											<p:commandButton icon="ui-icon-zoomin"
												onclick="PF('DialogViewGroupeActivite').show()"
												update=":formDialogViewGroupeActivite"
												actionListener="#{groupeActiviteBean.detailsGroupeActivite()}"
												disabled="#{groupeActiviteBean.headerButtonsDisabled}" />

											<p:commandButton icon="ui-icon-plus"
												onclick="PF('DialogAddGroupeActivite').show();"
												update=":formDialogAddGroupeActivite"
												actionListener="#{groupeActiviteBean.commandAddGroupe()}" />

											<p:commandButton icon="ui-icon-pencil"
												onclick="PF('DialogEditGroupeActivite').show();"
												update=":formDialogEditGroupeActivite"
												actionListener="#{groupeActiviteBean.commandEditGroupe()}"
												disabled="#{groupeActiviteBean.headerButtonsDisabled}" />

											<p:commandButton icon="ui-icon-trash" ajax="true"
												action="#{groupeActiviteBean.supprimerGroupeAct()}"
												update="groupeActiviteDataTable"
												disabled="#{groupeActiviteBean.headerButtonsDisabled}" />

										</f:facet>

										<p:column headerText="Code" style="text-align: center;">
											<h:outputText value="#{grpAct.codegrpa}" />
										</p:column>

										<p:column headerText="Libellé" style="text-align: center;">
											<h:outputText value="#{grpAct.libelles.get(0).lib}" />
										</p:column>

										<p:column headerText="Date effet" style="text-align: center;">
											<h:outputText value="#{grpAct.dateff}" />
										</p:column>

										<p:column headerText="Date fin" style="text-align: center;">
											<h:outputText value="#{grpAct.datefin}" />
										</p:column>
									</p:dataTable>
								</p:panel>

								<!-- //////////  Fin Gestion groupes activités ////////// -->

							</div>

							<div class="tab-pane fade" id="tab3">

								<!-- //////////  Debut Gestion des Imputations ////////// -->

								<p:panel header="Imputations" style="border:0px">
									<p:dataTable id="imputaDataTable"
										value="#{imputationBean.imputations}" var="imputa"
										selection="#{imputationBean.selectedImputation}"
										rowKey="#{imputa.idi}" selectionMode="single" paginator="true"
										paginatorTemplate="{PreviousPageLink} {NextPageLink} {RowsPerPageDropdown}"
										rowsPerPageTemplate="3,5,10" paginatorPosition="top" rows="5">

										<p:ajax event="rowSelect"
											listener="#{imputationBean.onRowSelectDataTable()}"
											update="imputaDataTable" />

										<f:facet name="header">

											<p:commandButton icon="ui-icon-zoomin"
												onclick="PF('DialogViewImputa').show()"
												update=":formDialogViewImputa"
												disabled="#{imputationBean.headerButtonsDisabled}" />

											<p:commandButton icon="ui-icon-plus"
												onclick="PF('DialogAddImputa').show();"
												update=":formDialogAddImputa" />

											<p:commandButton icon="ui-icon-pencil"
												onclick="PF('DialogEditImputa').show();"
												update=":formDialogEditImputa"
												disabled="#{imputationBean.headerButtonsDisabled}" />

											<p:commandButton icon="ui-icon-trash" ajax="true"
												action="#{imputationBean.supprimerImputation()}"
												update="imputaDataTable"
												disabled="#{imputationBean.headerButtonsDisabled}" />

										</f:facet>

										<p:column headerText="Code" style="text-align: center;">
											<h:outputText value="#{imputa.codei}" />
										</p:column>
										<p:column headerText="Libellé" style="text-align: center;">
											<h:outputText value="#{imputa.libelles.get(0).lib}" />
										</p:column>
										<p:column headerText="Compte" style="text-align: center;">
											<h:outputText value="#{imputa.imputaCompte.code}" />
										</p:column>
										<p:column headerText="Sous Compte" style="text-align: center;">
											<h:outputText value="#{imputa.imputaSousCompte.code}" />
										</p:column>
										<p:column headerText="Code Facturation"
											style="text-align: center;">
											<h:outputText value="#{imputa.imputaCodeFact.code}" />
										</p:column>
										<p:column headerText="Centre Coût" style="text-align: center;">
											<h:outputText value="#{imputa.imputaCentreCout.code}" />
										</p:column>

									</p:dataTable>
								</p:panel>

								<!-- //////////  Fin Gestion des Imputations ////////// -->

								<!-- ************************************************************************************ -->

								<!-- //////////  Debut Gestion des Imputa Compte ////////// -->

								<p:panel header="Comptes" style="border:0px">
									<p:dataTable id="imputaCompteDataTable"
										value="#{imputaCompteBean.imputaComptes}" var="imputaCompte"
										selection="#{imputaCompteBean.selectedImputaCompte}"
										rowKey="#{imputaCompte.id}" selectionMode="single"
										paginator="true"
										paginatorTemplate="{PreviousPageLink} {NextPageLink} {RowsPerPageDropdown}"
										rowsPerPageTemplate="3,5,10" paginatorPosition="top" rows="5">

										<p:ajax event="rowSelect"
											listener="#{imputaCompteBean.onRowSelectDataTable()}"
											update="imputaCompteDataTable" />

										<f:facet name="header">

											<p:commandButton icon="ui-icon-zoomin"
												onclick="PF('DialogViewImputaCompte').show()"
												update=":formDialogViewImputaCompte"
												disabled="#{imputaCompteBean.headerButtonsDisabled}" />

											<p:commandButton icon="ui-icon-plus"
												onclick="PF('DialogAddImputaCompte').show();"
												update=":formDialogAddImputaCompte" />

											<p:commandButton icon="ui-icon-pencil"
												onclick="PF('DialogEditImputaCompte').show();"
												update=":formDialogEditImputaCompte"
												disabled="#{imputaCompteBean.headerButtonsDisabled}" />

											<p:commandButton icon="ui-icon-trash" ajax="true"
												action="#{imputaCompteBean.supprimerImputaCompte()}"
												update="imputaCompteDataTable"
												disabled="#{imputaCompteBean.headerButtonsDisabled}" />

										</f:facet>

										<p:column headerText="Code" style="text-align: center;">
											<h:outputText value="#{imputaCompte.code}" />
										</p:column>
										<p:column headerText="Libellé" style="text-align: center;">
											<h:outputText value="#{imputaCompte.libelles.get(0).lib}" />
										</p:column>
									</p:dataTable>
								</p:panel>

								<!-- //////////  Fin Gestion des Imputa Compte ////////// -->

								<!-- ************************************************************************************ -->

								<!-- //////////  Debut Gestion des Imputa Sous Compte ////////// -->

								<p:panel header="Sous Comptes" style="border:0px">
									<p:dataTable id="imputaSousCompteDataTable"
										value="#{imputaSousCompteBean.imputaSousComptes}"
										var="imputaSousCompte"
										selection="#{imputaSousCompteBean.selectedImputaSousCompte}"
										rowKey="#{imputaSousCompte.id}" selectionMode="single"
										paginator="true"
										paginatorTemplate="{PreviousPageLink} {NextPageLink} {RowsPerPageDropdown}"
										rowsPerPageTemplate="3,5,10" paginatorPosition="top" rows="5">

										<p:ajax event="rowSelect"
											listener="#{imputaSousCompteBean.onRowSelectDataTable()}"
											update="imputaSousCompteDataTable" />


										<f:facet name="header">

											<p:commandButton icon="ui-icon-zoomin"
												onclick="PF('DialogViewImputaSousCompte').show()"
												update=":formDialogViewImputaSousCompte"
												disabled="#{imputaSousCompteBean.headerButtonsDisabled}" />

											<p:commandButton icon="ui-icon-plus"
												onclick="PF('DialogAddImputaSousCompte').show();"
												update=":formDialogAddImputaSousCompte" />

											<p:commandButton icon="ui-icon-pencil"
												onclick="PF('DialogEditImputaSousCompte').show();"
												update=":formDialogEditImputaSousCompte"
												disabled="#{imputaSousCompteBean.headerButtonsDisabled}" />

											<p:commandButton icon="ui-icon-trash" ajax="true"
												action="#{imputaSousCompteBean.supprimerImputaSousCompte()}"
												update="imputaSousCompteDataTable"
												disabled="#{imputaSousCompteBean.headerButtonsDisabled}" />

										</f:facet>

										<p:column headerText="Code" style="text-align: center;">
											<h:outputText value="#{imputaSousCompte.code}" />
										</p:column>
										<p:column headerText="Libellé" style="text-align: center;">
											<h:outputText value="#{imputaSousCompte.libelles.get(0).lib}" />
										</p:column>
									</p:dataTable>
								</p:panel>

								<!-- //////////  Fin
													   Gestion des Imputa Sous Compte ////////// -->

								<!-- ************************************************************************************ -->

								<!-- //////////  Debut Gestion des Imputa Code Facturation ////////// -->

								<p:panel header="Codes Facturation" style="border:0px">
									<p:dataTable id="imputaCodeFactDataTable"
										value="#{imputaCodeFactBean.imputaCodeFacts}"
										var="imputaCodeFact"
										selection="#{imputaCodeFactBean.selectedImputaCodeFact}"
										rowKey="#{imputaCodeFact.id}" selectionMode="single"
										paginator="true"
										paginatorTemplate="{PreviousPageLink} {NextPageLink} {RowsPerPageDropdown}"
										rowsPerPageTemplate="3,5,10" paginatorPosition="top" rows="5">

										<p:ajax event="rowSelect"
											listener="#{imputaCodeFactBean.onRowSelectDataTable()}"
											update="imputaCodeFactDataTable" />

										<f:facet name="header">

											<p:commandButton icon="ui-icon-zoomin"
												onclick="PF('DialogViewImputaCodeFact').show()"
												update=":formDialogViewImputaCodeFact"
												disabled="#{imputaCodeFactBean.headerButtonsDisabled}" />

											<p:commandButton icon="ui-icon-plus"
												onclick="PF('DialogAddImputaCodeFact').show();"
												update=":formDialogAddImputaCodeFact" />

											<p:commandButton icon="ui-icon-pencil"
												onclick="PF('DialogEditImputaCodeFact').show();"
												update=":formDialogEditImputaCodeFact"
												disabled="#{imputaCodeFactBean.headerButtonsDisabled}" />

											<p:commandButton icon="ui-icon-trash" ajax="true"
												action="#{imputaCodeFactBean.supprimerImputaCodeFact()}"
												update="imputaCodeFactDataTable"
												disabled="#{imputaCodeFactBean.headerButtonsDisabled}" />

										</f:facet>

										<p:column headerText="Code" style="text-align: center;">
											<h:outputText value="#{imputaCodeFact.code}" />
										</p:column>
										<p:column headerText="Libellé" style="text-align: center;">
											<h:outputText value="#{imputaCodeFact.libelles.get(0).lib}" />
										</p:column>
									</p:dataTable>
								</p:panel>


								<!-- //////////  Fin  Gestion des Imputa Code Facturation ////////// -->

								<!-- ************************************************************************************ -->

								<!-- //////////  Debut Gestion des Imputa Centre Cout ////////// -->

								<p:panel header="Centres de Coût" style="border:0px">
									<p:dataTable id="imputaCentreCoutDataTable"
										value="#{imputaCentreCoutBean.imputaCentreCouts}"
										var="imputaCentreCout"
										selection="#{imputaCentreCoutBean.selectedImputaCentreCout}"
										rowKey="#{imputaCentreCout.id}" selectionMode="single"
										paginator="true"
										paginatorTemplate="{PreviousPageLink} {NextPageLink} {RowsPerPageDropdown}"
										rowsPerPageTemplate="3,5,10" paginatorPosition="top" rows="5">

										<p:ajax event="rowSelect"
											listener="#{imputaCentreCoutBean.onRowSelectDataTable()}"
											update="imputaCentreCoutDataTable" />


										<f:facet name="header">

											<p:commandButton icon="ui-icon-zoomin"
												onclick="PF('DialogViewImputaCentreCout').show()"
												update=":formDialogViewImputaCentreCout"
												disabled="#{imputaCentreCoutBean.headerButtonsDisabled}" />

											<p:commandButton icon="ui-icon-plus"
												onclick="PF('DialogAddImputaCentreCout').show();"
												update=":formDialogAddImputaCentreCout" />

											<p:commandButton icon="ui-icon-pencil"
												onclick="PF('DialogEditImputaCentreCout').show();"
												update=":formDialogEditImputaCentreCout"
												disabled="#{imputaCentreCoutBean.headerButtonsDisabled}" />

											<p:commandButton icon="ui-icon-trash" ajax="true"
												action="#{imputaCentreCoutBean.supprimerImputaCentreCout()}"
												update="imputaCentreCoutDataTable"
												disabled="#{imputaCentreCoutBean.headerButtonsDisabled}" />

										</f:facet>

										<p:column headerText="Code" style="text-align: center;">
											<h:outputText value="#{imputaCentreCout.code}" />
										</p:column>
										<p:column headerText="Libellé" style="text-align: center;">
											<h:outputText value="#{imputaCentreCout.libelles.get(0).lib}" />
										</p:column>
									</p:dataTable>
								</p:panel>

								<!-- //////////  Fin Gestion des Imputa Centre cout ////////// -->
							</div>

							<div class="tab-pane fade" id="tab4">

								<!-- //////////  Debut Gestion groupes d'imputations ////////// -->

								<p:panel header="Groupes d'imputations" id="panelAct"
									style="border:0px;">
									<p:dataTable id="groupeImputaDataTable"
										value="#{groupeImputationBean.groupeImputations}" var="grpImp"
										selection="#{groupeImputationBean.selectedGroupeImputation}"
										rowKey="#{grpImp.idgrp}" selectionMode="single"
										paginator="true"
										paginatorTemplate="{PreviousPageLink} {NextPageLink} {RowsPerPageDropdown}"
										rowsPerPageTemplate="3,5,10" paginatorPosition="top" rows="5">

										<p:ajax event="rowSelect"
											listener="#{groupeImputationBean.onRowSelectDataTable()}"
											update="groupeImputaDataTable" />

										<f:facet name="header">

											<p:commandButton icon="ui-icon-zoomin"
												onclick="PF('DialogViewGroupeImputa').show()"
												update=":formDialogViewGroupeImputa"
												actionListener="#{groupeImputationBean.detailsGroupeImputation()}"
												disabled="#{groupeImputationBean.headerButtonsDisabled}" />

											<p:commandButton icon="ui-icon-plus"
												onclick="PF('DialogAddGroupeImputa').show();"
												update=":formDialogAddGroupeImputa"
												actionListener="#{groupeImputationBean.commandAddGroupe()}" />

											<p:commandButton icon="ui-icon-pencil"
												onclick="PF('DialogEditGroupeImputa').show();"
												update=":formDialogEditGroupeImputa"
												actionListener="#{groupeImputationBean.commandEditGroupe()}"
												disabled="#{groupeImputationBean.headerButtonsDisabled}" />

											<p:commandButton icon="ui-icon-trash" ajax="true"
												action="#{groupeImputationBean.supprimerGroupeImputation()}"
												update="groupeImputaDataTable"
												disabled="#{groupeImputationBean.headerButtonsDisabled}" />

										</f:facet>

										<p:column headerText="Code" style="text-align: center;">
											<h:outputText value="#{grpImp.codegrp}" />
										</p:column>

										<p:column headerText="Libellé" style="text-align: center;">
											<h:outputText value="#{grpImp.libelles.get(0).lib}" />
										</p:column>

										<p:column headerText="Date effet" style="text-align: center;">
											<h:outputText value="#{grpImp.dateeffe}" />
										</p:column>

										<p:column headerText="Date fin" style="text-align: center;">
											<h:outputText value="#{grpImp.datefin}" />
										</p:column>
									</p:dataTable>
								</p:panel>

								<!-- //////////  Fin Gestion groupes imputations ////////// -->
