<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"> 
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:p="http://primefaces.org/ui"> 

<ui:composition template="/template/MainContent.xhtml">
<ui:define name="Content">
	<p:panel header="Tranche">
		<h:form>
			<h:panelGrid columns="2" style="margin-bottom:10px" cellpadding="5">
						<p:outputLabel value="Code: " />
					    <p:inputText value="#{trancheBean.tranche.code}" required="true" /> 
					    <p:outputLabel value="Libelle: " />
					    <p:inputText value="#{trancheBean.tranche.libelle}" required="true" /> 
					    <h:outputLabel for="from" value="De:" />
            			<p:inputMask id="from" value="#{trancheBean.tranche.debut}" mask="99/99/9999">
		                <f:convertDateTime pattern="dd/MM/yyyy" />
		            	</p:inputMask>
			            <h:outputLabel for="to" value="A:" />
			            <p:inputMask id="to" value="#{trancheBean.tranche.fin}" mask="99/99/9999">
			                <f:convertDateTime pattern="dd/MM/yyyy" />
			            </p:inputMask>
			 </h:panelGrid>
			 <h:commandButton id="btn" value="Ajouter" type="submit" action="#{trancheBean.ajouter()}"/>
			 </h:form>
			 

</p:panel>

</ui:define>
</ui:composition>
</html>
